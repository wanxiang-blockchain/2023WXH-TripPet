{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "lib": ["DOM", "ES6", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "module": "ES6",
    "esModuleInterop": true,
    "sourceMap": false,
    "rootDir": "src",
    "outDir": "dist/js",
    "noEmitOnError": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["./src/__tests__/*"]
}
